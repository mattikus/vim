colorscheme mustang

if has("gui_macvim")
  set guifont=Menlo:h12
  set antialias
else
  set guifont=Terminus\ 12
  set noantialias
endif
set colorcolumn=+1
"set cursorline
"set relativenumber
"set number
set guioptions-=r
set guioptions-=R
set guioptions-=l
set guioptions-=L
set guioptions-=T
set guioptions-=m
set cmdheight=2
